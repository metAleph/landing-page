    "start": "npm run build-css && npm run build-js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metAleph/metaleph.com.git"
  },
  "keywords": [
    "metaleph",
    "webpage"
  ],
  "author": "Daniel Llatas SPiers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/metAleph/metaleph.com/issues"
  },
  "homepage": "https://github.com/metAleph/metaleph.com",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "handlebars": "^4.0.5",
    "jquery": "^2.1.4",
    "source-map": "0.5.3"
  },
  "devDependencies": {
    "browserify": "^9.0.3",
    "parcelify": "^0.12.5"
  },
  "browser": {
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js"
  },
  "style": [
    "./node_modules/bootstrap/dist/css/bootstrap.min.css",
    "./css/main.css",
